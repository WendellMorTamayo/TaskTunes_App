    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <color name="white_40_percent_opacity">#19FFFFFF</color>
    <dimen name="mediaplayerbar_height">64dp</dimen>
    <dimen name="navigation_bar_height">54dp</dimen>
